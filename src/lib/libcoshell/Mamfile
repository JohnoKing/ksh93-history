exec - proto -p -s -l /home/gsf/arch/sgi.mips3/lib/package/ast.lic '-o since=1990,author=gsf' coshell.h > 1.${COTEMP}.x
exec - if	cmp 2>/dev/null -s ${INSTALLROOT}/include/ast/coshell.h 1.${COTEMP}.x
exec - then	rm -f 1.${COTEMP}.x
