info mam static 00000 1994-07-17 make (AT&T Research) 4.0 2000-02-01
setv INSTALLROOT ../../..
setv PACKAGE_ast ${INSTALLROOT}
setv PACKAGE_ast_INCLUDE ${PACKAGE_ast}/include
setv PACKAGE_ast_LIB ${PACKAGE_ast}/lib
setv PACKAGEROOT ../../../../..
setv AR ar
setv ARFLAGS cr
setv AS as
setv ASFLAGS
setv CC cc
setv CCFLAGS "-O ${mam_cc_DLL}"
setv COTEMP $$
setv CPIO cpio
setv CPIOFLAGS
setv CPP "${CC} -E"
setv F77 f77
setv HOSTCC ${CC}
setv IGNORE
setv LD ld
setv LDFLAGS 
setv LEX lex
setv LEXFLAGS
setv LPR lpr
setv LPRFLAGS
setv M4FLAGS 
setv MAKE nmake
setv MAKEFLAGS
setv PR pr
setv PRFLAGS
setv SHELL /bin/sh
setv SILENT
setv TAR tar
setv YACC yacc
setv YACCFLAGS -d
make ${PACKAGE_ast_LIB}/package/ast.lic
done ${PACKAGE_ast_LIB}/package/ast.lic
make install
make dll
make libdll.a archive
make dll.req
exec - echo 'main(){return(0);}' > 1.${COTEMP}.c
exec - ${CC} ${CCFLAGS}    -c 1.${COTEMP}.c &&
exec - x=`${CC} ${CCFLAGS}    ${LDFLAGS}  -o 1.${COTEMP}.x 1.${COTEMP}.o -l'*' 2>&1 | sed -e 's/[][()+@?]/#/g' || :` &&
exec - {
exec - case "" in
exec - *?)	echo " " ;;
exec - esac
exec - for i in dll dl dld ast
exec - do	case $i in
exec - 	"dll"|dll)
exec - 		;;
exec - 	*)	if	test ! -f ${INSTALLROOT}/lib/lib$i.a
exec - 		then	case `{ ${CC} ${CCFLAGS}    -L. ${LDFLAGS}  -o 1.${COTEMP}.x 1.${COTEMP}.o  -l$i 2>&1 || echo '' $x ;} | sed -e 's/[][()+@?]/#/g' || :` in
exec - 			*$x*)	case `{ ${CC} ${CCFLAGS}    ${LDFLAGS}  -o 1.${COTEMP}.x 1.${COTEMP}.o  -l$i 2>&1 || echo '' $x ;} | sed -e 's/[][()+@?]/#/g' || :` in
exec - 				*$x*) continue ;;
exec - 				esac
exec - 				;;
exec - 			esac
exec - 		fi
exec - 		;;
exec - 	esac
exec - 	echo " -l$i"
exec - done
exec - } > dll.req
exec - rm -f 1.${COTEMP}.*
done dll.req generated
make dlfcn.o
make dlfcn.c
make dll.h implicit
done dll.h dontcare virtual
make xcoff.h implicit
done xcoff.h dontcare virtual
make dl.h implicit
done dl.h dontcare virtual
make ${PACKAGE_ast_INCLUDE}/ast/error.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/option.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/ast.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/sfio.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/ast_common.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_common.h dontcare
make ${PACKAGE_ast_INCLUDE}/ast/ast_std.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/ast_botch.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_botch.h dontcare
make ${PACKAGE_ast_INCLUDE}/ast/ast_unistd.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_unistd.h dontcare
make ${PACKAGE_ast_INCLUDE}/ast/re_comp.h implicit
make ${PACKAGE_ast_INCLUDE}/prototyped.h implicit
done ${PACKAGE_ast_INCLUDE}/prototyped.h dontcare
done ${PACKAGE_ast_INCLUDE}/ast/re_comp.h dontcare
make ${PACKAGE_ast_INCLUDE}/ast/getopt.h implicit
prev ${PACKAGE_ast_INCLUDE}/prototyped.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/getopt.h dontcare
make ${PACKAGE_ast_INCLUDE}/ast/ast_fcntl.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/ls.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/ast_mode.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_mode.h dontcare
make ${PACKAGE_ast_INCLUDE}/ast/ast_fs.h implicit
make error?--?old?ethernet?address?at?wrong?offset implicit
done error?--?old?ethernet?address?at?wrong?offset dontcare virtual
done ${PACKAGE_ast_INCLUDE}/ast/ast_fs.h dontcare
prev ${PACKAGE_ast_INCLUDE}/ast/ast_std.h implicit
prev ${PACKAGE_ast_INCLUDE}/prototyped.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ls.h dontcare
prev ${PACKAGE_ast_INCLUDE}/ast/ast_fs.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_fcntl.h dontcare
make ${PACKAGE_ast_INCLUDE}/ast/ast_types.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_types.h dontcare
make ${PACKAGE_ast_INCLUDE}/ast/stdio.h implicit
prev ${PACKAGE_ast_INCLUDE}/ast/getopt.h implicit
prev ${PACKAGE_ast_INCLUDE}/ast/ast_common.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/stdio.h dontcare
prev ${PACKAGE_ast_INCLUDE}/ast/getopt.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/limits.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/limits.h dontcare
make ${PACKAGE_ast_INCLUDE}/ast/ast_hdr.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_hdr.h dontcare
prev ${PACKAGE_ast_INCLUDE}/ast/getopt.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/ast_lib.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_lib.h dontcare
prev ${PACKAGE_ast_INCLUDE}/ast/ast_common.h implicit
prev ${PACKAGE_ast_INCLUDE}/prototyped.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_std.h dontcare
done ${PACKAGE_ast_INCLUDE}/ast/sfio.h dontcare
prev ${PACKAGE_ast_INCLUDE}/ast/ast_std.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/ast_version.h implicit
prev ${PACKAGE_ast_INCLUDE}/prototyped.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_version.h dontcare
prev ${PACKAGE_ast_INCLUDE}/prototyped.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast.h dontcare
prev ${PACKAGE_ast_INCLUDE}/prototyped.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/option.h dontcare
prev ${PACKAGE_ast_INCLUDE}/ast/ast.h implicit
prev ${PACKAGE_ast_INCLUDE}/prototyped.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/error.h
make dlldefs.h implicit
make FEATURE/dll
make features/dll
done features/dll
prev dll.req
setv mam_libast ${mam_cc_L+-last}${mam_cc_L-${PACKAGE_ast_LIB}/libast.a}
exec - iffe -v -c '${CC} ${CCFLAGS}    ${LDFLAGS} '  ref ${mam_cc_L+-L${PACKAGE_ast_LIB}} -I${PACKAGE_ast_INCLUDE}/ast -I${PACKAGE_ast_INCLUDE} ${mam_libast} ${mam_libast} : run features/dll
done FEATURE/dll generated
exec - cmp 2>/dev/null -s FEATURE/dll dlldefs.h || { rm -f dlldefs.h; cp FEATURE/dll dlldefs.h; }
done dlldefs.h generated
prev ${PACKAGE_ast_INCLUDE}/ast/ast.h implicit
done dlfcn.c
prev dlfcn.c
exec - ${CC} ${CCFLAGS}  -I. -I${PACKAGE_ast_INCLUDE}/ast -I${PACKAGE_ast_INCLUDE} -D_BLD_dll -D_PACKAGE_ast -c dlfcn.c
done dlfcn.o generated
make dllfind.o
make dllfind.c
make ${PACKAGE_ast_INCLUDE}/ast/fts.h implicit
prev ${PACKAGE_ast_INCLUDE}/ast/ls.h implicit
prev ${PACKAGE_ast_INCLUDE}/prototyped.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/fts.h
prev dlldefs.h implicit
prev ${PACKAGE_ast_INCLUDE}/ast/ast.h implicit
done dllfind.c
prev dllfind.c
exec - ${CC} ${CCFLAGS}  -I. -I${PACKAGE_ast_INCLUDE}/ast -I${PACKAGE_ast_INCLUDE} -D_HOSTTYPE_=\""${_hosttype_=`package`}"\" -D_BLD_dll -D_PACKAGE_ast -c dllfind.c
done dllfind.o generated
make dllnext.o
make dllnext.c
make link.h implicit
done link.h dontcare virtual
prev dlldefs.h implicit
prev ${PACKAGE_ast_INCLUDE}/ast/ast.h implicit
done dllnext.c
prev dllnext.c
exec - ${CC} ${CCFLAGS}  -I. -I${PACKAGE_ast_INCLUDE}/ast -I${PACKAGE_ast_INCLUDE} -D_BLD_dll -D_PACKAGE_ast -c dllnext.c
done dllnext.o generated
make dlllook.o
make dlllook.c
prev dlldefs.h implicit
prev ${PACKAGE_ast_INCLUDE}/ast/ast.h implicit
done dlllook.c
prev dlllook.c
exec - ${CC} ${CCFLAGS}  -I. -I${PACKAGE_ast_INCLUDE}/ast -I${PACKAGE_ast_INCLUDE} -D_BLD_dll -D_PACKAGE_ast -c dlllook.c
done dlllook.o generated
exec - ${AR} cr libdll.a dlfcn.o dllfind.o dllnext.o dlllook.o
exec - (ranlib libdll.a) >/dev/null 2>&1 || true
done libdll.a generated
done dll virtual
prev libdll.a archive
make ${PACKAGE_ast_LIB}
exec - set -
exec - if	test ! -d ${PACKAGE_ast_LIB}
exec - then	set -x && mkdir -p ${PACKAGE_ast_LIB} 2>/dev/null && set - ||
exec - 	{
exec - 		test -d ./-p && rm -rf ./-p
exec - 		p=
exec - 		for d in `echo ${PACKAGE_ast_LIB} | sed -e 's%\\(.\\)/%\\1 %g'`
exec - 		do	case $p in
exec - 			"")	p=$d ;;
exec - 			*)	p=$p/$d ;;
exec - 			esac
exec - 			case $d in
exec - 			.|..)	;;
exec - 			*)	if	test ! -d $p
exec - 				then	rm -f $p
exec - 					set -x && mkdir $p && set -
exec - 				fi
exec - 				;;
exec - 			esac
exec - 		done
exec - 	} && set -x 		    		   
exec - fi
done ${PACKAGE_ast_LIB} generated
make ${PACKAGE_ast_LIB}/libdll.a archive
prev libdll.a archive
exec - mv ${PACKAGE_ast_LIB}/libdll.a ${PACKAGE_ast_LIB}/libdll.a.old 2>/dev/null || true; cp libdll.a ${PACKAGE_ast_LIB}/libdll.a
exec - (ranlib ${PACKAGE_ast_LIB}/libdll.a) >/dev/null 2>&1 || true
done ${PACKAGE_ast_LIB}/libdll.a generated
make ${PACKAGE_ast_LIB}/lib
exec - set -
exec - if	test ! -d ${PACKAGE_ast_LIB}/lib
exec - then	set -x && mkdir -p ${PACKAGE_ast_LIB}/lib 2>/dev/null && set - ||
exec - 	{
exec - 		test -d ./-p && rm -rf ./-p
exec - 		p=
exec - 		for d in `echo ${PACKAGE_ast_LIB}/lib | sed -e 's%\\(.\\)/%\\1 %g'`
exec - 		do	case $p in
exec - 			"")	p=$d ;;
exec - 			*)	p=$p/$d ;;
exec - 			esac
exec - 			case $d in
exec - 			.|..)	;;
exec - 			*)	if	test ! -d $p
exec - 				then	rm -f $p
exec - 					set -x && mkdir $p && set -
exec - 				fi
exec - 				;;
exec - 			esac
exec - 		done
exec - 	} && set -x 		    		   
exec - fi
done ${PACKAGE_ast_LIB}/lib generated
make ${PACKAGE_ast_LIB}/lib/dll
prev dll.req
exec - mv ${PACKAGE_ast_LIB}/lib/dll ${PACKAGE_ast_LIB}/lib/dll.old 2>/dev/null || true; cp dll.req ${PACKAGE_ast_LIB}/lib/dll
done ${PACKAGE_ast_LIB}/lib/dll generated
make ${PACKAGE_ast_INCLUDE}/ast
exec - set -
exec - if	test ! -d ${PACKAGE_ast_INCLUDE}/ast
exec - then	set -x && mkdir -p ${PACKAGE_ast_INCLUDE}/ast 2>/dev/null && set - ||
exec - 	{
exec - 		test -d ./-p && rm -rf ./-p
exec - 		p=
exec - 		for d in `echo ${PACKAGE_ast_INCLUDE}/ast | sed -e 's%\\(.\\)/%\\1 %g'`
exec - 		do	case $p in
exec - 			"")	p=$d ;;
exec - 			*)	p=$p/$d ;;
exec - 			esac
exec - 			case $d in
exec - 			.|..)	;;
exec - 			*)	if	test ! -d $p
exec - 				then	rm -f $p
exec - 					set -x && mkdir $p && set -
exec - 				fi
exec - 				;;
exec - 			esac
exec - 		done
exec - 	} && set -x 		    		   
exec - fi
done ${PACKAGE_ast_INCLUDE}/ast generated
make ${PACKAGE_ast_INCLUDE}/ast/dlldefs.h
prev dlldefs.h
exec - proto -p -s -l ${PACKAGE_ast_LIB}/package/ast.lic -o since='1997,author=gsf' dlldefs.h > 1.${COTEMP}.x
exec - if	cmp 2>/dev/null -s ${PACKAGE_ast_INCLUDE}/ast/dlldefs.h 1.${COTEMP}.x
exec - then	rm -f 1.${COTEMP}.x
exec - else	mv 1.${COTEMP}.x ${PACKAGE_ast_INCLUDE}/ast/dlldefs.h
exec - fi
done ${PACKAGE_ast_INCLUDE}/ast/dlldefs.h generated
done install virtual
make test
done test dontcare virtual
