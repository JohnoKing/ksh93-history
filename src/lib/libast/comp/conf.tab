CHILD_MAX			POSIX	SC 1 LMUX	6	cc{
	#if _lib_fork
	#define child()		fork()
	#else
	#ifdef	_map_spawnve
	#define spawnve		_map_spawnve
	#endif
	extern int		spawnve(const char*, char* const[], char* const[]);
	static char*		cmd[] = { "/bin/echo", 0 };
	#define child()		spawnve(cmd[0],cmd,(char**)0)
	#endif
	int main()
	{
		int	i;
		int	n;
		for (i = 3; i < 512 && (n = child()) >= 0; i++)
			if (!n) _exit(0);
		while (wait((int*)0) > 0);
		for (n = 8; n < i; n <<= 1);
		printf("%d", n);
		return 0;
	}
}
